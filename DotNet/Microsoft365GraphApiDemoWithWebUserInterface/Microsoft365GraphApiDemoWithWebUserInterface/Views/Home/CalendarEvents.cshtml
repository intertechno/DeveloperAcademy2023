@model Dictionary<string, string>
@{
    ViewData["Title"] = "Calendar Events";
}

<h2>Calendar events</h2>
<table class="table w-50">
    <thead>
        <tr>
            <th>Subject</th>
            <th>Start Date</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var eventItem in Model)
        {
            <tr>
                <td>@eventItem.Key</td>
                <td>@eventItem.Value</td>
            </tr>
        }
    </tbody>
</table>
